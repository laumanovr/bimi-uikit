{
  "name": "ui-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "yarn run build:lib && yarn run build:style",
    "build:lib": "vue-cli-service build --target lib --name ui-kit src/index.js",
    "build:style": "sass src/styles/main.scss dist/styles.css --style compressed",
    "lint": "vue-cli-service lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o storybook"
  },
  "dependencies": {
    "bootstrap": "5.2.3",
    "core-js": "^3.8.3",
    "sass": "1.57.1",
    "sass-loader": "13.2.0",
    "vue": "2.6.14"
  },
  "devDependencies": {
    "@babel/core": "7.12.16",
    "@babel/eslint-parser": "7.12.16",
    "@storybook/addon-actions": "6.5.15",
    "@storybook/addon-essentials": "6.5.15",
    "@storybook/addon-interactions": "6.5.15",
    "@storybook/addon-links": "6.5.15",
    "@storybook/builder-webpack5": "6.5.15",
    "@storybook/manager-webpack5": "6.5.15",
    "@storybook/testing-library": "0.0.13",
    "@storybook/vue": "6.5.15",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "babel-loader": "8.3.0",
    "css-loader": "6.7.3",
    "eslint": "7.32.0",
    "eslint-plugin-vue": "8.0.3",
    "style-loader": "3.3.1",
    "vue-loader": "15.10.1",
    "vue-template-compiler": "2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
